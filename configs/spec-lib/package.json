{
  "name": "@the-dev-tools/spec-lib",
  "private": true,
  "type": "module",
  "exports": {
    "./core": {
      "typespec": "./src/core/main.tsp",
      "types": "./dist/src/core/index.d.ts",
      "default": "./dist/src/core/index.js"
    },
    "./data-client": {
      "typespec": "./src/data-client/main.tsp",
