{
  "name": "@the-dev-tools/api-recorder-extension",
  "displayName": "API Recorder",
  "author": "dev.tools",
  "version": "0.4.7",
  "scripts": {
    "build": "plasmo build --build-path=dist --zip",
    "dev": "plasmo dev --build-path=dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "@effect/schema": "0.74.0",
    "@plasmohq/storage": "1.12.0",
    "effect": "3.8.3",
    "magic-sdk": "28.10.0",
    "plasmo": "0.89.2",
    "react": "18.3.1",
    "react-aria-components": "1.3.3",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "tailwind-merge": "2.5.2",
    "tailwind-variants": "0.2.1",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@the-dev-tools/config-eslint": "workspace:^",
    "@the-dev-tools/config-tailwind": "workspace:^",
    "@the-dev-tools/config-typescript": "workspace:^",
    "@the-dev-tools/ui": "workspace:^",
    "@the-dev-tools/utils": "workspace:^",
    "@types/chrome": "~0.0.271",
    "@types/node": "~22.5.5",
    "@types/react": "~18.3.8",
    "@types/react-dom": "~18.3.0",
    "@types/uuid": "~10.0.0",
    "autoprefixer": "~10.4.20",
    "devtools-protocol": "~0.0.1358005",
    "postcss": "~8.4.47",
    "tailwindcss": "~3.4.13",
    "typescript": "~5.6.2",
    "typescript-eslint": "~8.6.0"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "debugger",
      "tabs",
      "unlimitedStorage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/auth-callback.html"
        ],
        "matches": [
          "*://*.magic.link/*"
        ]
      }
    ]
  }
}
