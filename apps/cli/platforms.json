{
  "darwin-amd64": {
    "go": {
      "CGO_ENABLED": "1",
      "GOOS": "darwin",
      "GOARCH": "amd64"
    }
  },
  "darwin-arm64": {
    "go": {
      "CGO_ENABLED": "1",
      "GOOS": "darwin",
      "GOARCH": "arm64"
    }
  },
  "linux-x64": {
    "go": {
      "CGO_ENABLED": "1",
      "GOOS": "linux",
      "GOARCH": "amd64"
    }
  },
  "linux-arm64": {
    "go": {
      "CC": "aarch64-linux-gnu-gcc",
      "CXX": "aarch64-linux-gnu-g++",
      "CGO_ENABLED": "1",
      "CGO_CFLAGS": "-O2 -g",
      "CGO_CXXFLAGS": "-O2 -g",
      "CGO_LDFLAGS": "-O2 -g",
      "GOOS": "linux",
      "GOARCH": "arm64"
    }
  },
  "win32-x64": {
    "binary-suffix": ".exe",
    "go": {
      "GOOS": "windows",
      "GOARCH": "amd64",
      "CC": "x86_64-w64-mingw32-gcc",
      "CGO_ENABLED": "1",
      "CXX": "x86_64-w64-mingw32-g++"
    }
  },
  "win32-ia32": {
    "binary-suffix": ".exe",
    "go": {
      "CGO_ENABLED": "1",
      "GOOS": "windows",
      "GOARCH": "386",
      "CC": "i686-w64-mingw32-gcc",
      "CXX": "i686-w64-mingw32-g++"
    }
  }
}
