{
  "name": "@the-dev-tools/api-recorder-extension",
  "displayName": "API Recorder",
  "author": "dev.tools",
  "version": "0.4.10",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "plasmo build --build-path=dist --zip",
    "dev": "plasmo dev --build-path=dist"
  },
  "dependencies": {
    "@plasmohq/storage": "1.15.0",
    "effect": "3.14.6",
    "magic-sdk": "29.0.5",
    "plasmo": "0.90.3",
    "react": "19.1.0",
    "react-aria-components": "1.7.1",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "tailwind-merge": "3.2.0",
    "tailwind-variants": "1.0.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "~4.1.3",
    "@the-dev-tools/eslint-config": "workspace:^",
    "@the-dev-tools/ui": "workspace:^",
    "@types/chrome": "~0.0.313",
    "@types/node": "~22.14.0",
    "@types/react": "~19.1.0",
    "@types/react-dom": "~19.1.1",
    "devtools-protocol": "~0.0.1443917",
    "postcss": "~8.5.3",
    "tailwindcss": "~4.1.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.29.1"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "debugger",
      "tabs",
      "unlimitedStorage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/auth-callback.html"
        ],
        "matches": [
          "*://*.magic.link/*"
        ]
      }
    ]
  }
}
