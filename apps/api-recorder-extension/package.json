{
  "name": "@the-dev-tools/api-recorder-extension",
  "displayName": "API Recorder",
  "author": "dev.tools",
  "version": "0.4.10",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "plasmo build --build-path=dist --zip",
    "dev": "plasmo dev --build-path=dist"
  },
  "dependencies": {
    "@plasmohq/storage": "1.13.0",
    "effect": "3.12.0",
    "magic-sdk": "28.20.0",
    "plasmo": "0.89.4",
    "react": "19.0.0",
    "react-aria-components": "1.5.0",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "tailwind-merge": "2.6.0",
    "tailwind-variants": "0.3.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@the-dev-tools/config-eslint": "workspace:^",
    "@the-dev-tools/config-tailwind": "workspace:^",
    "@the-dev-tools/ui": "workspace:^",
    "@the-dev-tools/utils": "workspace:^",
    "@types/chrome": "~0.0.287",
    "@types/node": "~22.10.0",
    "@types/react": "~19.0.2",
    "@types/react-dom": "~19.0.2",
    "autoprefixer": "~10.4.20",
    "devtools-protocol": "~0.0.1399977",
    "postcss": "~8.4.49",
    "tailwindcss": "~3.4.15",
    "typescript": "~5.7.2",
    "typescript-eslint": "~8.18.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "debugger",
      "tabs",
      "unlimitedStorage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/auth-callback.html"
        ],
        "matches": [
          "*://*.magic.link/*"
        ]
      }
    ]
  }
}
