    "@data-client/endpoint": "0.14.26"
  },
  "devDependencies": {
    "@alloy-js/cli": "~0.18.0",
    "@alloy-js/core": "~0.18.2",
    "@alloy-js/typescript": "~0.18.0",
    "@bufbuild/protobuf": "~2.6.2",
    "@the-dev-tools/eslint-config": "workspace:^",
    "@types/node": "~24.1.0",
    "@typespec/emitter-framework": "~0.9.0",
    "effect": "~3.17.1",
    "prettier": "~3.6.2",
    "typescript": "~5.8.3"
  }
}
